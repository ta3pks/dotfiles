{
	"snippet.highlight": true,
	"pullDiagnostic.onSave": true,
	"sql.database": "postgresql",
	"tailwindCSS.enable": true,
	"zig.path": "/opt/homebrew/bin/zig",
	"tailwindCSS.cssLanguages": [
		"css",
		"less",
		"postcss",
		"sass",
		"scss",
		"stylus",
		"vue"
	],
	"tailwindCSS.htmlLanguages": [
		"html",
		"rust",
	],
	"list.source.extensions.defaultAction": "toggle",
	"pullDiagnostic.workspace": true,
	"rust-analyzer.inlayHints.chainingHints.enable": true,
	"suggest.removeDuplicateItems": true,
	"signature.enable": true,
	"diagnostic.messageTarget": "float",
	"diagnostic.virtualTextCurrentLineOnly": false,
	"inlayHint.display": true,
	"rust-analyzer.inlayHints.typeHints.hideNamedConstructor": true,
	"rust-analyzer.inlayHints.closureStyle": "rust_analyzer",
	"Lua.hint.arrayIndex": "Disable",
	"inlayHint.enableParameter": true,
	"inlayHint.refreshOnInsertMode": false,
	"rust-analyzer.inlayHints.typeHints.enable": true,
	"rust-analyzer.rust.analyzerTargetDir": true,
	"rust-analyzer.imports.prefix": "crate",
	"rust-analyzer.imports.merge.glob": true,
	"rust-analyzer.imports.group.enable": true,
	"rust-analyzer.imports.granularity.group": "crate",
	"rust-analyzer.imports.granularity.enforce": true,
	"coc.preferences.formatOnSave": true,
	"diagnostic.virtualText": true,
	"diagnostic.locationlistLevel": "error",
	"diagnostic.locationlistUpdate": true,
	"coc.preferences.useQuickfixForLocations": true,
	"coc.preferences.enableMessageDialog": true,
	"coc.source.buffer.ignoreGitignore": true,
	"rust-analyzer.check.command": "clippy",
	"rust-analyzer.checkOnSave": true,
	"rust-analyzer.workspace.symbol.search.kind": "all_symbols",
	"rust-analyzer.workspace.symbol.search.limit": 128,
	"rust-analyzer.workspace.symbol.search.scope": "workspace_and_dependencies",
	"rust-analyzer.cargo.features": "all",
	"rust-analyzer.check.features": "all",
	"rust-analyzer.cargo.buildScripts.enable": true,
	"rust-analyzer.check.allTargets": true,
	"rust-analyzer.procMacro.enable": true,
	"rust-analyzer.diagnostics.disabled": [
		"inactive-code",
		"needless_return",
	],
	"Lua.runtime.version": "LuaJIT",
	"sumneko-lua.enableNvimLuaDev": true,
	"Lua.workspace.library": [
		"/opt/homebrew/Cellar/neovim/0.9.4/share/nvim/runtime",
		"/Users/nikos/.hammerspoon/Spoons/EmmyLua.spoon/annotations",
	],
	"coc.preferences.extensionUpdateCheck": "daily",
	"svelte.enable-ts-plugin": true,
	"suggest.insertMode": "insert",
	"vimlsp.suggest.fromRuntimepath": true,
	"languageserver": {
		"elmLS": {
			"command": "elm-language-server",
			"filetypes": [
				"elm"
			],
			"rootPatterns": [
				"elm.json"
			]
		}
	},
	"emmet.includeLanguages": {
		"svelte": "html",
		"javascript": "javascriptreact",
		"rust": "html",
	},
	//	"codeLens.enable": true,
}
