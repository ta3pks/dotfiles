cmd - h : yabai -m window --focus west
ctrl + cmd - l : yabai -m space --focus recent
ctrl + cmd - k : yabai -m space --focus prev
ctrl + cmd - j : yabai -m space --focus next
ctrl + cmd - left : yabai -m window --space prev; yabai -m space --focus prev
ctrl + cmd - right : yabai -m window --space next; yabai -m space --focus next
shift + cmd - 0x1E : yabai -m display --focus next
shift + cmd - 0x21 : yabai -m display --focus prev
ctrl + cmd - 0x1E : yabai -m window --focus east
ctrl + cmd - 0x21 : yabai -m window --focus west
ctrl + cmd - q 	  : yabai -m space --destroy
ctrl + cmd - c 	  : yabai -m space --create; yabai -m space --focus next
# send window to monitor and follow focus
# ctrl + cmd - c  : yabai -m window --display next; yabai -m display --focus next
# ctrl + cmd - 1  : yabai -m window --display 1; yabai -m display --focus 1

# move floating window
# shift + ctrl - a : yabai -m window --move rel:-20:0
# shift + ctrl - s : yabai -m window --move rel:0:20

# increase window size
#cmd + alt - 0x1E : yabai -m window --resize left:-20:0
#cmd + alt - [ : yabai -m window --resize top:0:-20

# decrease window size
#cmd + cmd - } : yabai -m window --resize bottom:0:-20
#cmd + cmd - { : yabai -m window --resize top:0:20

# set insertion point in focused container
#ctrl + alt - h : yabai -m window --insert west

# toggle window zoom
#alt - d : yabai -m window --toggle zoom-parent
#ctrl+cmd - f : yabai -m window --toggle zoom-fullscreen

# toggle window split type
cmd+ctrl - e : yabai -m window --toggle split

# float / unfloat window and center on screen
ctrl+cmd - t : yabai -m window --toggle float --grid 4:4:1:1:2:2

# toggle sticky(+float), picture-in-picture
# alt - p : yabai -m window --toggle sticky --toggle pip
